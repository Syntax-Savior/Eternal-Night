    <section class="section products" aria-labelledby="products-label">
      <div class="container">

        <div class="title-wrapper">

          <div>
            <h2 class="section-title headline-small">Enjoy protection from payment to delivery</h2>

            <p class="section-text body-large">
              Get curated benefits, such as exclusive discounts, enhanced
              protection, and extra support, to help grow your business
              every step of the way.
            </p>
          </div>

          <a href="#" class="btn btn-outline">
            <span class="label-medium">Learn more</span>

            <span class="material-symbols-rounded" aria-hidden="true">arrow_outward</span>
          </a>

        </div>

        <div class="products-list">

          @if (Model == null || !Model.Any())
          {
            <p>MODEL IS NULL OR EMPTY. Separate statement to find out which.</p>
          }
          else
          {
            @foreach (var product in Model)
            {
              <div class="card">

                <div class="card-banner">

                  <a class="product" data-spm="product-1" href="/Product/ProductDescriptionPage/@product.Id" target="_blank" data-spm-protocol="i" data-appeared="true">
                    <figure class="img-holder" style="--width: 585; --height: 390;">
                      <img src="@product.ImageUrl" alt="@product.Title" width="585" height="390" class="img-cover">
                    </figure>
                  </a>

                  @if (product.IsNew)
                  {
                    <span class="badge label-medium">New</span>
                  }

                  <button class="icon-btn fav-btn" aria-label="add to favorite" data-toggle-btn>
                    <span class="material-symbols-rounded" aria-hidden="true">favorite</span>
                  </button>

                  <a class="cart-link" data-spm="" href="#" target="_blank" data-spm-protocol="" data-appeared="true">
                    <button class="icon-btn cart-btn" aria-label="add to cart">
                      <span class="material-symbols-outlined" aria-hidden="true">add_shopping_cart</span>
                    </button>
                  </a>

                </div>

                <div class="card-content">

                  <h3 class="title-small card-title" title="@product.Title">
                    <a href="/Product/ProductDescriptionPage/@product.Id">@product.Title</a>
                  </h3>

                  <div class="product-review">

                    <div class="rating-wrapper">
                      <span class="material-symbols-rounded" aria-hidden="true">star</span>
                      <span class="material-symbols-rounded" aria-hidden="true">star</span>
                      <span class="material-symbols-rounded" aria-hidden="true">star</span>
                      <span class="material-symbols-rounded" aria-hidden="true">star</span>
                      <span class="material-symbols-rounded" aria-hidden="true">star_half</span>
                    </div>

                    <span class="title-small rating-text">@product.SoldCount sold</span>

                    @if (product.TopSelling)
                    {
                      <img src="/Assets/Images/Icons/Top-Selling-7Days.png" alt="Top Selling in 7 Days" class="top-selling-7days">
                    }

                  </div>

                  <div class="product-price">

                    <div class="discounted">
                      <span class="currency">@product.Currency</span>
                      <span class="amount">@Math.Floor(product.Price)</span>
                      <span class="decimal-point">.</span>
                      <span class="decimals">@((product.Price % 1).ToString("F2").Substring(2))</span>
                    </div>

                    <div class="original">
                      <span class="striked-through">@product.Currency @product.OriginalPrice</span>
                    </div>

                  </div>

                  <div class="discount-details">

                    <img src="/Assets/Images/Icons/Super-Deals.png" alt="Super Deals" class="super-deals">

                    <span class="divider"></span>

                    <img src="/Assets/Images/Icons/Black-Friday.png" alt="Black Friday" class="black-friday">

                    <span class="divider"></span>

                    <img src="/Assets/Images/Icons/Discount-Arrow.png" alt="Discount Arrow" class="discount-arrow">

